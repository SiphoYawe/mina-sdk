{
  "name": "@siphoyawe/mina-sdk",
  "version": "0.0.2",
  "description": "Cross-chain bridge SDK for Hyperliquid - Bridge assets from 40+ chains with auto-deposit to your trading account",
  "author": "Mina Bridge Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/siphoyawe/mina-sdk"
  },
  "homepage": "https://github.com/siphoyawe/mina-sdk#readme",
  "bugs": {
    "url": "https://github.com/siphoyawe/mina-sdk/issues"
  },
  "keywords": [
    "hyperliquid",
    "bridge",
    "cross-chain",
    "defi",
    "sdk",
    "crypto",
    "evm",
    "lifi"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
