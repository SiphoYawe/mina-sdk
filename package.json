{
  "name": "@siphoyawe/mina-sdk",
  "version": "1.3.1",
  "description": "Cross-chain bridge SDK for Hyperliquid - Bridge assets from 40+ chains with auto-deposit to your trading account",
  "author": "Mina Bridge Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/siphoyawe/mina-sdk"
  },
  "homepage": "https://github.com/siphoyawe/mina-sdk#readme",
  "bugs": {
    "url": "https://github.com/siphoyawe/mina-sdk/issues"
  },
  "keywords": [
    "hyperliquid",
    "bridge",
    "cross-chain",
    "defi",
    "sdk",
    "crypto",
    "evm",
    "lifi",
    "react",
    "hooks",
    "provider",
    "web3",
    "ethereum",
    "typescript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.mts",
        "default": "./dist/react.mjs"
      },
      "require": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
